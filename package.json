{
  "name": "flights-map",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "1.0.17"
  },
  "dependencies": {
    "@gooddata/react-components": "^2.9.2",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "4.0.0-beta.3",
    "react": "15",
    "react-dom": "15",
    "react-google-maps": "^9.2.2",
    "react-redux": "^5.0.6",
    "react-select": "^2.0.0-alpha.1",
    "react-table": "^6.7.6",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^4.8.0"
  },
  "proxy": {
    "/gdc": {
      "changeOrigin": true,
      "cookieDomainRewrite": "localhost",
      "target": "https://secure.gooddata.com/"
    },
    "/json": {
      "changeOrigin": true,
      "cookieDomainRewrite": "localhost",
      "target": "https://flightxml.flightaware.com/"
    },
    "/data": {
      "changeOrigin": true,
      "cookieDomainRewrite": "localhost",
      "target": "https://api.openweathermap.org/"
    },
    "/*.html": {
      "changeOrigin": true,
      "secure": false,
      "target": "https://secure.gooddata.com/"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
